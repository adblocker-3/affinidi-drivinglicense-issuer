={showNavbar ? 'show' : ''}>\n          <Nav className=\"justify-content-end\">\n            {appState.isAuthenticated && (\n              <>\n                <Nav.Item>\n                  <Nav.Link as={'span'} className='user-logout'>\n                    <UserLogout />\n                  </Nav.Link>\n                </Nav.Item>\n                <Nav.Item>\n                  <Nav.Link as={'span'}>\n                    <span>| Hello, {appState.username}</span>\n                  </Nav.Link>\n                </Nav.Item>\n              </>\n            )}\n\n            {!appState.isAuthenticated && (\n              <>\n                <Nav.Item>\n                  <Nav.Link as={NavLink} activeClassName='is-active' to={routes.APPLICANT_LOGIN}>Start applying Driving License</Nav.Link>\n                </Nav.Item>\n                <Nav.Item>\n                  <Nav.Link as={NavLink} activeClassName='is-active' to={routes.ISSUER_LOGIN}>Sign in as admin</Nav.Link>\n                </Nav.Item>\n                <Nav.Item>\n                  <Nav.Link as={NavLink} activeClassName='is-active' to={routes.API_KEY}>API Key</Nav.Link>\n                </Nav.Item>\n              </>\n            )}\n          </Nav>\n        </Navbar.Collapse>\n      </Container>\n    </Navbar>\n  )\n}\n\nexport default LayoutHeaderNavigation\n","/Users/benjamin/Projects/affinidi/affinidi-drivinglicense-issuer/src/utils/config.ts",[],"/Users/benjamin/Projects/affinidi/affinidi-drivinglicense-issuer/src/constants/localstorage.ts",[],"/Users/benjamin/Projects/affinidi/affinidi-drivinglicense-issuer/src/constants/endpoints.ts",[],"/Users/benjamin/Projects/affinidi/affinidi-drivinglicense-issuer/src/constants/routes.ts",[],"/Users/benjamin/Projects/affinidi/affinidi-drivinglicense-issuer/src/pages/issuer/Issuer.tsx",[],"/Users/benjamin/Projects/affinidi/affinidi-drivinglicense-issuer/src/pages/not-found/NotFound.tsx",[],"/Users/benjamin/Projects/affinidi/affinidi-drivinglicense-issuer/src/pages/intro/